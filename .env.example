# Exemplo de variáveis de ambiente para produção (Vercel)
# Copie estas variáveis para as Environment Variables na Vercel

# Banco de Dados PostgreSQL
DATABASE_URL=postgresql://usuario:senha@host:5432/database?schema=schema

# JWT Secret (use uma chave forte e única)
JWT_SECRET=sua_chave_secreta_muito_forte_e_aleatoria

# Ambiente
NODE_ENV=production

# Base Path da aplicação
BASE_PATH=/pfo-ronda

# CORS - Domínios permitidos (separados por vírgula)
CORS_ORIGIN=https://lmedu.com.br

# Porta (apenas para desenvolvimento local, Vercel ignora)
PORT=3001
